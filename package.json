    "bardjs": "^0.1.8",
    "chai": "^3.5.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.10.2",
    "eslint-plugin-angular": "^1.4.1",
    "eslint-plugin-disable": "^0.3.0",
