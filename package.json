  "author": "Jared Abbott",
  "main": "server.js",
  "scripts": {
    "install-ui": "node dev/task/install-ui.js",
    "compile": "node dev/task/compile.js",
    "delint-css": "node_modules/stylelint/bin/stylelint.js app/**/*.scss",
    "delint-js": "node_modules/eslint/bin/eslint.js --fix app/ && node_modules/eslint/bin/eslint.js --fix dev/task/",
    "delint": "npm run delint-js && npm run delint-css",
    "dev": "gulp dev",
    "docs": "gulp docs",
    "server": "node server.js",
    "setup": "nvm i 6.9.4 && nvm use && npm i && cp .envrc .env && gulp compile",
    "start": "npm i && cp .envrc .env && gulp compile",
    "test": "node dev/task/compile.js && node_modules/.bin/karma start karma.conf.js"
  },
  "dependencies": {
    "angular": "1.5.5",
    "angular-animate": "1.5.5",
    "angular-aria": "1.5.5",
    "angular-messages": "1.5.5",
