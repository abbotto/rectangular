    "angular-sanitize": "1.5.5",
    "angular-translate": "^2.13.0",
    "angular-ui-router": "^0.3.2",
    "animate.css": "^3.5.2",
    "bluebird": "^3.4.6",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.16.6",
    "moment": "^2.16.0",
    "ng-file-upload": "^12.2.13",
    "normalize.css": "^5.0.0",
