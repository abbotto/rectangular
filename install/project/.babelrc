{
	"compact": true,
	"presets": [
		[
			"es2015",
			{"modules": false}
		],
		"react",
		["env", {
			"targets": {
				"browsers": [
					"last 2 versions",
					"safari >= 7",
					"ie >= 9"
				]
			}
		}]
	],
	"plugins": [
		["transform-runtime", {
			"helpers": false,
			"polyfill": false,
			"regenerator": true,
			"moduleName": "babel-runtime"
		}]
	]
}
