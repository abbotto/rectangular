{
	"compact": true,
	"presets": [
		"react",
		["env", {
			"targets": {
				"browsers": [
					"last 3 versions",
					"safari >= 7",
					"ie >= 9"
				]
			}
		}]
	],
	"plugins": [
		["transform-runtime", {
			"helpers": false,
			"polyfill": true,
			"regenerator": true,
			"moduleName": "babel-runtime"
		}]
	]
}
